{
    "name": "gammadia/translator",
    "description": "Translation component, with builtin fallback for french language, and variable replacement",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": "^7.1 || ^8.0",
        "phpmyadmin/motranslator": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "overtrue/phplint": "^2.4 || ^3.0 || ^4.0",
        "phpstan/phpstan": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Gammadia\\i18n\\": "src/Gammadia/i18n/",
            "Gammadia\\i18n\\Tests\\": "tests/"
         }
    },
    "minimum-stability": "stable"
}
