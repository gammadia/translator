{
    "name": "gammadia/translator",
    "description": "Translation component, with builtin fallback for french language, and variable replacement",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "phpmyadmin/motranslator": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "overtrue/phplint": "^2.4|^3.0",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "Gammadia\\i18n\\": "src/Gammadia/i18n/",
            "Gammadia\\i18n\\Tests\\": "tests/"
         }
    },
    "minimum-stability": "stable"
}
